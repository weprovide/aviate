{
  "name": "aviate",
  "version": "1.0.6",
  "files": [
    "lib",
    "cli.js"
  ],
  "scripts": {
    "lint": "xo",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "xo",
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
